{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": ["dom", "esnext"],
      "allowJs": true,
      "jsx": "preserve",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
  }
  